<h1>Post List</h1>
<%= link_to 'Add new Post', new_post_path, class: 'btn btn-success' %>

<table>
  <thead>
  <tr>
    <td>title</td>
    <td>content</td>
    <td>created at</td>
    <td>update at</td>
  </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= post.created_at %></td>
      <td><%= post.updated_at %></td>
    </tr>
  <% end %>
  </tbody>
</table>
